{
  "version": 2,
  "name": "micafe-api",
  "builds": [
    {
      "src": "dist/api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"],
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "dist/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
