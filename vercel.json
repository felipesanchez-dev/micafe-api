{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["docs/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CACHE_TTL_MS": "300000",
    "SCRAPING_TIMEOUT_MS": "10000",
    "MAX_RETRIES": "3",
    "RETRY_DELAY_MS": "1000",
    "FEDERACION_CAFETEROS_URL": "https://federaciondecafeteros.org",
    "LOG_LEVEL": "info"
  }
}
